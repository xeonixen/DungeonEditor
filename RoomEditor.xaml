<Window x:Class="DungeonEditorV2.RoomEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DungeonEditorV2"
        mc:Ignorable="d"
        Title="RoomEditor" Height="300" Width="300">
    <Grid>
        <Menu><MenuItem Header="_File">
            <MenuItem Header="_Import" Click="MenuImportClick"/>
            <MenuItem Header="_Open" Click="MenuOpenClick"/>
            <MenuItem Header="_Save" Click="MenuSaveClick"></MenuItem>
        </MenuItem></Menu>
        <Canvas x:Name="BaseCanvas" Margin="0,30,0,0"></Canvas>
    </Grid>
</Window>
